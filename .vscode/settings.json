{
  "fileheader.configObj": {
    "createFileTime": true,
    "language": {
      "gitattributes": {
        "head": "### ",
        "middle": "# @",
        "end": "### "
      },
      "gitignore": {
        "head": "### ",
        "middle": "## @",
        "end": "### "
      },
      "md": {
        "head": "<!--",
        "middle": " * @",
        "end": " * @License: CC-BY-NC-SA_V4_0 or any later version \n -->"
      }
    },
    "autoAdd": true,
    "autoAddLine": 100,
    "autoAlready": true,
    "annotationStr": {
      "head": "/*",
      "middle": " * @",
      "end": " */",
      "use": false
    },
    "headInsertLine": {
      "php": 2,
      "py": 0,
      "java": 0,
      "c": 0,
      "cpp": 0,
      "sh": 0,
      "h": 0
    },
    "beforeAnnotation": {
      "py": "#!/usr/bin/env python3\n# coding=utf-8",
      "sh": "#!/bin/bash\nset -eoux pipefail"
    },
    "afterAnnotation": {},
    "specialOptions": {},
    "switch": {
      "newlineAddAnnotation": true
    },
    "prohibitAutoAdd": [
      "json",
      "md",
      "wav",
      "xml",
      "java"
    ],
    "moveCursor": true,
    "dateFormat": "YYYY-MM-DD HH:mm:ss",
    "atSymbol": "@",
    "atSymbolObj": {},
    "colon": ": ",
    "colonObj": {},
    "filePathColon": "路径分隔符替换",
    "showErrorMessage": false,
    "wideSame": false,
    "wideNum": 13,
    "CheckFileChange": false
  },
  "fileheader.customMade": {
    "Github": "https://github.com/Certseeds/CS203_DSAA_template_java",
    "Organization": "SUSTech",
    "Author": "nanoseeds",
    "Date": "Do not edit",
    "LastEditors": "nanoseeds",
    "LastEditTime": "Do not edit"
  },
  "todo-tree.general.tags": [
    "TODO:",
    "FIXME:"
  ],
  "todo-tree.highlights.defaultHighlight": {
    "gutterIcon": true
  },
  "todo-tree.highlights.customHighlight": {
    "TODO:": {
      "foreground": "#fff",
      "background": "#ffbd2a",
      "iconColour": "#ffbd2a"
    },
    "FIXME:": {
      "foreground": "#fff",
      "background": "#f06292",
      "icon": "flame",
      "iconColour": "#f06292"
    }
  },
  "files.associations": {
    "sshd_config": "bat",
    "tuple": "cpp",
    "limits": "cpp",
    "fstream": "cpp"
  }
}